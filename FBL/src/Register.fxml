<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RegisterController">
   <children>
      <VBox prefHeight="80.0" prefWidth="800.0" stylesheets="@Register.css">
          <styleClass>
              <String fx:value="topbar" />
          </styleClass>
         <children>
            <Button mnemonicParsing="false" onAction="#back" prefHeight="30.0" prefWidth="40.0" text="&lt;">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <VBox layoutX="200.0" layoutY="80.0" prefHeight="520.0" prefWidth="600.0">
         <children>
            <TextField promptText="First name">
               <VBox.margin>
                  <Insets bottom="20.0" left="50.0" right="40.0" top="50.0" />
               </VBox.margin>
            </TextField>
            <TextField layoutX="260.0" layoutY="30.0" promptText="Last name">
               <VBox.margin>
                  <Insets bottom="40.0" left="50.0" right="40.0" />
               </VBox.margin>
            </TextField>
            <TextField layoutX="260.0" layoutY="79.0" promptText="Username">
               <VBox.margin>
                  <Insets bottom="20.0" left="50.0" right="40.0" />
               </VBox.margin>
            </TextField>
            <PasswordField promptText="Password">
               <VBox.margin>
                  <Insets bottom="40.0" left="50.0" right="40.0" />
               </VBox.margin>
            </PasswordField>
            <TextField layoutX="260.0" layoutY="79.0" promptText="Current age">
               <VBox.margin>
                  <Insets bottom="10.0" left="50.0" right="40.0" />
               </VBox.margin>
            </TextField>
         </children>
      </VBox>
      <VBox alignment="TOP_RIGHT" layoutY="80.0" prefHeight="520.0" prefWidth="200.0">
         <children>
            <Label alignment="TOP_LEFT" text="First name" textAlignment="RIGHT">
               <font>
                  <Font size="20.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" top="50.0" />
               </VBox.margin>
            </Label>
         </children>
         <padding>
            <Insets right="50.0" top="50.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
